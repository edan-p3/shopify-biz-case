{
  "version": 2,
  "name": "shopify-business-case",
  "builds": [
    {
      "src": "app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/app/dist/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app/dist/index.html"
    }
  ],
  "framework": null
}
